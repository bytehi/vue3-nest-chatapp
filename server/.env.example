NODE_ENV=development
PORT=3000

# 数据库
DATABASE_TYPE=mysql
DATABASE_HOST=localhost
DATABASE_PORT=3306
DATABASE_USERNAME=your_username
DATABASE_PASSWORD=your_password
DATABASE_NAME=admin
DATABASE_SYNC=false
DATABASE_LOGGING=true
DATABASE_POOL_SIZE=10

# Redis
REDIS_URL=redis://localhost:6379
REDIS_DB=0
REDIS_PREFIX=nest:
REDIS_TTL=86400

# HTTP客户端
HTTP_PORT=3000
HTTP_TIMEOUT=5000
HTTP_MAX_REDIRECTS=5
HTTP_RETRIES=3
HTTP_RETRY_DELAY=1000
HTTP_MAX_CONCURRENT=10

# JWT
JWT_SECRET=your-jwt-secret
JWT_EXPIRES_IN=1d
JWT_REFRESH_SECRET=your-refresh-secret
JWT_REFRESH_EXPIRES_IN=7d

# API
API_PREFIX=api
API_VERSION=v1

# 日志
LOG_LEVEL=info
LOG_FILE_PATH=logs/app.log
LOG_MAX_FILES=30
LOG_MAX_SIZE=20m

# 安全
ALLOWED_ORIGINS=http://localhost:3000
CSRF_SECRET=your-csrf-secret
SESSION_SECRET=your-session-secret
COOKIE_DOMAIN=localhost
COOKIE_SECURE=false

# 上传
UPLOAD_DEST=./uploads
UPLOAD_MAX_SIZE=5242880
UPLOAD_MAX_FILES=10
UPLOAD_ALLOWED_TYPES=image/*,application/pdf

# 百度千帆API
QIANFAN_API_KEY=your_api_key
QIANFAN_API_SECRET=your_api_secret

# 初始密码
INITIAL_PASSWORD=Aa12345678
# 初始头像
INITIAL_AVATAR=http://groupavatar.test.file.mediportal.com.cn/0b4d0ac4627646948f83341ec302f56a
